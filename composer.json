{
    "name": "mtownsend/laravel-make-trait",
    "description": "Rapid trait generation in Laravel.",
    "keywords": [
        "laravel",
        "trait",
        "artisan",
        "make"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Townsend",
            "email": "mtownsend5512@gmail.com",
            "role": "Developer"
        }
    ],
	"autoload": {
        "psr-4": {
            "Mtownsend\\MakeTrait\\": "src"
        }
    },
    "require": {
        "php": "~7.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|7.0|^8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|7.0|^8.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Mtownsend\\MakeTrait\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtownsend\\MakeTrait\\Providers\\MakeTraitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
